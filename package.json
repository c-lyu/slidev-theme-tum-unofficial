{
  "name": "slidev-theme-tum-unofficial",
  "description": "An unofficial TUM presentation theme for Slidev",
  "version": "0.1.0",
  "author": {
      "name": "C. Lyu",
      "website": "https://github.com/c-lyu"
  },
  "repository": "https://github.com/c-lyu/slidev-theme-tum-unofficial",
  "type": "module",
  "private": true,
  "keywords": [
      "slidev-theme",
      "slidev"
  ],
  "slidev": {
    "colorSchema": "light"
  },
  "main": "example.md",
  "scripts": {
      "build": "slidev build example.md",
      "dev": "slidev example.md --open",
      "export": "slidev export example.md",
      "screenshot": "slidev export example.md --format pdf",
      "lint": "eslint --fix --ext .js,.ts,.vue ."
  },
  "dependencies": {
    "@slidev/cli": "^0.49.9",
    "@slidev/types": "^0.49.5",
    "@iconify-json/mdi": "^1.1.66",
    "@iconify-json/mingcute": "^1.1.17",
    "prism-theme-vars": "^0.2.5",
    "codemirror-theme-vars": "^0.1.2",
    "markdown-it-mark": "^4.0.0",
    "markdown-it-ins": "^4.0.0",
    "unocss": "^0.60.4",
    "vue": "^3.4.27"
  }
}